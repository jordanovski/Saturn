@model Saturn.Model.Codebooks.ContactType

@{
    ViewBag.Title = "Типови на контакти";
}

<h3>Дали сте сигурни дека сакате да го избришете следново?</h3>
<div>
    <h4>Тип на контакт</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.Type)</dt>
        <dd>@Html.DisplayFor(model => model.Type)</dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Избриши" class="btn btn-default" /> |
            @Html.ActionLink("Вратисе назад", "Index")
        </div>
    }

</div>

