@model IEnumerable<Saturn.Model.Codebooks.RegistrationStatus>

@{
    ViewBag.Title = "Статус на пријава";
}

<h2>@ViewBag.Title</h2>

@(Html.Kendo().Grid(Model)
    .Name("grid")
    .Columns(columns =>
    {
        columns.Bound(p => p.Status);
    })
    .DataSource(dataSource => dataSource
        .Ajax()
        .Read(read => read.Action("Read", "RegistrationStatus"))
    )
)
